#!/usr/bin/osascript
tell application "System Events"
	tell current location of network preferences
		set VPNservice to service "epasaule"
		set isConnected to connected of current configuration of VPNservice
		if isConnected then
			disconnect VPNservice
         return "Disconnected from epasaule"
		else
			connect VPNservice
         return "Connecting to epasaule..."
		end if
	end tell
end tell



